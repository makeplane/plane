{
  "extends": "@plane/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/app/*": ["app/*"],
      "@/*": ["core/*"],
      "@/public/*": ["public/*"],
      "@/plane-admin/*": ["ce/*"],
      "@/styles/*": ["styles/*"]
    },
    "strictNullChecks": true
  },
  "include": ["next-env.d.ts", "next.config.js", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
