{
  "extends": "@plane/typescript-config/react-router.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDirs": [".", "./.react-router/types"],
    "types": ["node", "vite/client"],
    "paths": {
      "@/app/*": ["app/*"],
      "@/*": ["core/*"],
      "@/plane-admin/*": ["ce/*"],
      "@/styles/*": ["styles/*"]
    },
    "strictNullChecks": true
  },
  "include": ["**/*", "**/.server/**/*", "**/.client/**/*", ".react-router/types/**/*"],
  "exclude": ["node_modules"]
}
