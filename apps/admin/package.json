    "mobx-react": "catalog:",
    "mobx-react-lite": "^4.0.7",
    "morgan": "^1.10.1",
    "next-themes": "^0.2.1",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "7.51.5",
    "react-router": "^7.9.1",
    "react-router-dom": "^7.9.1",
    "serve": "14.2.5",
