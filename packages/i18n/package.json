{
  "name": "@plane/i18n",
  "version": "2.0.0",
  "license": "AGPL-3.0",
  "description": "Internationalization for Plane",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "check:lint": "eslint . --cache --cache-location node_modules/.cache/eslint/ --max-warnings=0",
    "check:types": "tsc --noEmit",
    "check:format": "prettier . --cache --check",
    "i18n:check": "tsx scripts/check-missing-keys.ts",
    "i18n:migrate": "tsx scripts/migrate-translations.ts",
    "i18n:generate-types": "tsx scripts/generate-types.ts",
    "i18n:validate": "tsx scripts/validate-translations.ts",
    "fix:lint": "eslint . --cache --cache-location node_modules/.cache/eslint/ --fix",
    "fix:format": "prettier . --cache --write",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "i18next": "^25.7.3",
    "react": "catalog:"
  },
  "devDependencies": {
    "@plane/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "tsdown": "catalog:",
    "tsx": "^4.19.2",
    "typescript": "catalog:"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./server": "./dist/server/index.js",
    "./package.json": "./package.json"
  }
}
